<nav class="bg-dark text-light d-flex justify-content-end p-3 pr-5">
  <div class="w-25 d-flex justify-content-between">
    <a href='/'>Upload</a>
    <a href='/browse'>Images</a>
    <a href='/browseMemes'>Memes</a>
  </div>
</nav>

<div class="container pt-4">

  <h1>{{title}}</h1>
  <p>Welcome to {{title}}</p>

  <form action="/upload" method="post" enctype="multipart/form-data">
    Select image to upload:
    <input type="file" name="fileUpload" accept="image/jpg, image/gif, image/png" id="fileUpload" />
    <input type="submit" value="Upload Image" name="submit" />
  </form>

  {{#if error }}
  <strong>{{error}}</strong>
  {{/if}}

</div>